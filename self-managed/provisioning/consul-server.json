{
    "acl": {
        "enabled": true,
        "down_policy": "async-cache",
        "default_policy": "deny",
        "tokens": {
            "agent": "${token}",
            "default": "${token}",
            "initial_management": "${token}"
        }
    },
    "datacenter": "${datacenter}",
    "encrypt": "",
    "encrypt_verify_incoming": false,
    "encrypt_verify_outgoing": false,
    "server": true,
    "bootstrap": true,
    "log_level": "INFO",
    "ui_config": {
        "enabled": true
    },
    "tls": {
        "defaults": {
            "ca_file": "/etc/consul.d/ca.pem",
            "verify_outgoing": false
        }
    },
    "ports": {
        "grpc": 8502
    }
}